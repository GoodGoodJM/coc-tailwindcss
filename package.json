{
  "name": "coc-tailwindcss",
  "displayName": "Tailwind CSS IntelliSense",
  "description": "Tailwind CSS class name completion",
  "version": "0.2.0",
  "publisher": "iamcco",
  "engines": {
    "coc": "^0.0.46"
  },
  "keywords": [
    "coc.nvim",
    "tailwind",
    "tailwindcss",
    "css",
    "intellisense",
    "autocomplete"
  ],
  "activationEvents": [
    "workspaceContains:**/{tailwind,tailwind.config,tailwind-config,.tailwindrc}.js"
  ],
  "main": "./lib/index.js",
  "contributes": {
    "configuration": {}
  },
  "preview": true,
  "scripts": {
    "build": "rm -rf ./lib && tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^11.13.5",
    "coc.nvim": "^0.0.67",
    "color": "^3.1.0",
    "dlv": "^1.1.2",
    "tmp": "0.0.33"
  },
  "dependencies": {
    "tailwindcss-language-server": "0.0.1",
    "vscode-languageserver-protocol": "^3.14.1"
  },
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/iamcco/coc-tailwindcss/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/iamcco/coc-tailwindcss/issues",
    "email": "ooiss@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamcco/coc-tailwindcss.git"
  }
}
